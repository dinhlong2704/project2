<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.projectfinal.view.frg.M000SplashFrag"
        android:label="@string/splash"
        app:route="@string/splash"
        tools:layout="@layout/m000_splash">
        <action
            android:id="@+id/toLogin"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.projectfinal.view.frg.M001LoginFrag"
        android:label="@string/login"
        app:route="@string/login"
        tools:layout="@layout/m002_register" />

</navigation>